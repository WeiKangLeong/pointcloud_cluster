<launch>

	<node pkg="pointcloud_transform" type="pointcloud_readjust" name="right_readjust" output="screen" >
		<param name="x" value="0.0" />
		<param name="y" value="0.0" />
		<param name="z" value="0.0" />
		<param name="roll" value="0.0" />
		<param name="pitch" value="0.0" />
		<param name="yaw" value="0.0" />
		<param name="frame_id" value="robosense_right" />
		<param name="cut_half" value="false" />
		<remap from="velodyne_points" to="/golfcart/right_rs/rslidar_points" />
		<remap from="transform_points" to="right_lidar_points" />
	</node>
	<node pkg="pointcloud_transform" type="pointcloud_readjust" name="left_readjust" output="screen" >
		<param name="x" value="0.0" />
		<param name="y" value="0.0" />
		<param name="z" value="0.0" />
		<param name="roll" value="0.0" />
		<param name="pitch" value="0.0" />
		<param name="yaw" value="0.00" />
		<param name="frame_id" value="robosense_left" />
		<param name="cut_half" value="false" />
		<remap from="velodyne_points" to="/golfcart/left_rs/rslidar_points" />
		<remap from="transform_points" to="left_lidar_points" />
	</node>

	<node launch-prefix="nice" pkg="rviz" type="rviz" name="rviz" args="-d $(find pointcloud_transform)/calibration_left.rviz" />


	<node pkg="tf" type="static_transform_publisher" name="baselink_to_fake_baselink" args="4.753 1.3 0 0.505 0 0 golfcart/base_link golfcart/fake_base_link 100" />
	<node pkg="tf" type="static_transform_publisher" name="fake_baselink_to_left_rs" args="0 0 1.1577554 0 -0.0043171184 0.067041476 golfcart/fake_base_link robosense_left 100" />
	<node pkg="tf" type="static_transform_publisher" name="fake_baselink_to_right_rs" args="-1.150586 -2.1529434 1.1520137 -1.1082841 -0.0091039551 -0.044584437 golfcart/fake_base_link robosense_right 100" />


</launch>
