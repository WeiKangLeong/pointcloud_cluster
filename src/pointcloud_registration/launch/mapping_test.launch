<launch>
	<node pkg="pointcloud_registration" type="scan_accumulation" name="icp_map" output="screen" >
		<param name="odom_frame" value="wheelchair/odom" />
		<param name="base_frame" value="iMiev/base_link" />
		<param name="map_frame" value="iMiev/map" />
		<remap from="rslidar_points" to="filter_points" />
	</node>

	<node pkg="pointcloud_filter" type="pointcloud_filter_passthrough" name="filter_inside">
		<remap from="input" to="rslidar_points" />
		<remap from="output" to="filter_points" />
		<param name="filter_inside" value="true" />
		<param name="filter_outside" value="false" />
		<param name="filter_size" value="1.0" />
	</node>

</launch>
